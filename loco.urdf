  </gazebo>
  <!-- Defines hardware interface. -->
  <ros2_control name="SystemControl" type="system">
    <hardware>
      <plugin>ign_ros2_control/IgnitionSystem</plugin>
    </hardware>
    <joint name="waist">
      <command_interface name="position">
        <param name="min">-3.141582653589793</param>
        <param name="max">3.141582653589793</param>
      </command_interface>
      <state_interface name="position"/>
      <param name="initial_position">0</param>
    </joint>
    <joint name="shoulder">
      <command_interface name="position">
        <param name="min">-1.8849555921538759</param>
        <param name="max">1.9896753472735358</param>
      </command_interface>
      <state_interface name="position"/>
      <param name="initial_position">-1.1</param>
    </joint>
    <joint name="elbow">
      <command_interface name="position">
        <param name="min">-2.1467549799530254</param>
        <param name="max">1.6057029118347832</param>
      </command_interface>
      <state_interface name="position"/>
      <param name="initial_position">1.55</param>
    </joint>
    <joint name="forearm_roll">
      <command_interface name="position">
        <param name="min">-3.141582653589793</param>
        <param name="max">3.141582653589793</param>
      </command_interface>
      <state_interface name="position"/>
      <param name="initial_position">0</param>
    </joint>
    <joint name="wrist_angle">
      <command_interface name="position">
        <param name="min">-1.7453292519943295</param>
        <param name="max">2.1467549799530254</param>
      </command_interface>
      <state_interface name="position"/>
      <param name="initial_position">0.5</param>
    </joint>
    <joint name="wrist_rotate">
      <command_interface name="position">
        <param name="min">-3.141582653589793</param>
        <param name="max">3.141582653589793</param>
      </command_interface>
      <state_interface name="position"/>
      <param name="initial_position">0</param>
    </joint>
    <joint name="gripper">
      <command_interface name="position"/>
      <state_interface name="position"/>
    </joint>
    <joint name="left_finger">
      <command_interface name="position">
        <param name="min">-0.01</param>
        <param name="max">0.037</param>
      </command_interface>
      <state_interface name="position"/>
      <param name="initial_position">0.0195</param>
    </joint>
    <joint name="right_finger">
      <!-- <param name="mimic">left_finger</param>
            <param name="multiplier">1</param> -->
      <command_interface name="position">
        <param name="min">-0.037</param>
        <param name="max">0.01</param>
      </command_interface>
      <state_interface name="position"/>
      <param name="initial_position">-0.0195</param>
    </joint>
    <joint name="pan">
      <command_interface name="position"/>
      <state_interface name="position"/>
      <param name="initial_position">0</param>
      <!-- initial position for
      the GenericSystem -->
    </joint>
    <joint name="tilt">
      <command_interface name="position"/>
      <state_interface name="position"/>
      <param name="initial_position">0.8</param>
      <!-- initial position for
      the GenericSystem -->
    </joint>
    <!-- base_type ==
      'kobuki' -->
    <!-- TODO:(speralta): Move pan_and_tilt sensors here? -->
  </ros2_control>
</robot>
