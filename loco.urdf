      <robot_param>robot_description</robot_param>
      <parameters>
          /simply_ws/install/interbotix_xslocobot_sim/share/interbotix_xslocobot_sim/config/_version/locobot_wx250s_controllers.yaml
        </parameters>
    </plugin>
    <!-- Loads simulated sensors. -->
    <plugin filename="ignition-gazebo-sensors-system" name="ignition::gazebo::systems::Sensors">
    </plugin>
